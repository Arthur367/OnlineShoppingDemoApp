<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.arthur.onlineshoppingapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.arthur.onlineshoppingapp.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.arthur.onlineshoppingapp.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/cart"
        android:name="com.arthur.onlineshoppingapp.ui.home.HomeFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/categories"
        android:name="com.arthur.onlineshoppingapp.ui.gallery.GalleryFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/settings"
        android:name="com.arthur.onlineshoppingapp.ui.slideshow.SlideshowFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/logout"
        android:name="com.arthur.onlineshoppingapp.ui.slideshow.SlideshowFragment"
        android:label="fragment_logout"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/search"
        android:name="com.arthur.onlineshoppingapp.BlankFragment"
        android:label="search"
        tools:layout="@layout/blank_fragment" />
    <activity
        android:id="@+id/sellerHomeActivity"
        android:name="com.arthur.onlineshoppingapp.Seller.SellerHomeActivity"
        android:label="SellerHomeActivity" />
</navigation>